<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bjpowernode.demo.dao.StudentDao">
    <!--    按手机号查询student-->
    <select id="selectByPhone" resultType="com.bjpowernode.model.Student">
        select * from student where number=#{number}
    </select>
    
    <insert id="insertStudent">
        insert into student(name,number,age) values (#{name},#{number},#{age})
    </insert>
</mapper>